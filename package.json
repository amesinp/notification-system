    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amesinp/notification-system.git"
  },
  "author": "Precious Amesinlola",
